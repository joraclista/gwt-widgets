<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="WidgetsShowcase">


    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.googlecode.mgwt.dom.DOM"/>


    <entry-point class='com.github.joraclista.client.ui.showcase.WidgetsShowcase'/>



    <servlet path='/DouJobsService'
             class='com.github.joraclista.server.DouJobsServiceImpl'/>

    <source path='client'/>
    <source path='shared'/>

    <set-property name="user.agent" value="safari" />
</module>